{
  "name": "backend",
  "version": "1.0.0",
  "description": "Boilerplate",
  "main": "build/index.js",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register src/index.ts",
    "start": "node -r tsconfig-paths/register build/index.js",
    "build": "tsc",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "stress": "autocannon -c 100 -d 10 -p 10",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "seed": "ts-node -r tsconfig-paths/register src/core/utils/seeds/run-seeds.ts",
    "migration:generate": "npm run typeorm -- migration:generate src/migrations/CreateTables",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert"
  },
  "keywords": [],
  "author": "Eduardo Santarosa",
  "license": "MIT",
  "dependencies": {
    "@fastify/helmet": "^13.0.0",
    "@fastify/static": "^8.0.3",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "^5.2.0",
    "@fastify/websocket": "^11.0.1",
    "@google-cloud/translate": "^8.5.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "fastify": "^5.1.0",
    "html-pdf-node": "^1.0.8",
    "i18next": "^24.1.2",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.7.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "nodemailer": "^6.9.16",
    "nunjucks": "^3.2.4",
    "pg": "^8.13.1",
    "puppeteer": "^23.11.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bull": "^4.10.4",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.1",
    "@types/nodemailer": "^6.4.17",
    "@types/nunjucks": "^3.2.6",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0"
  }
}
